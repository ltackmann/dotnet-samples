<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <system.data>
  		<DbProviderFactories>
    		<add name="SQLite Data Provider"
          		invariant="Mono.Data.Sqlite"
          		description="Data Provider for SQLite"
          		type="Mono.Data.Sqlite.SqliteFactory, Mono.Data.Sqlite" />
  		</DbProviderFactories>
	</system.data>
	
	<connectionStrings>
  		<add name="UserContext"
        	connectionString="URI=file:SqliteTest.db"
        	providerName="Mono.Data.Sqlite" />
	</connectionStrings>
  </configSections>
</configuration>